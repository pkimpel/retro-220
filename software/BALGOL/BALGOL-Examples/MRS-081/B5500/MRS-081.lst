                      INTEGER ARRAY M[1], ROW[1];                                                           0000
                      BEGIN                                                                                 0000
                      DO BEGIN                                                                              0000
                        ROW[ROWX]:= (M[MX] DIV 1000000) MOD 1000;                                           0000
                        ROW[ROWX+1]:= (M[MX] DIV 1000) MOD 1000;                                            0003
                        ROW[ROWX+2]:= M[MX] MOD 1000;                                                       0007
                        ROWX:= ROWX+3;                                                                      0011
                        MX:= MX+1;                                                                          0012
                        COUNT:= COUNT-1;                                                                    0013
                        END                                                                                 0014
