2 COMMENT TEST PROGRAM 1 FOR BALGOL LIBRARY FUNCTIONS
2         2018-01-18 P.KIMPEL;
2
2 INTEGER DEGREES;
2 REAL RADS, TORADS;
2 REAL VSQRT, VSIN, VCOS, VTAN, VASIN, VACOS, VATAN;
2
2 OUTPUT RESULTS(DEGREES, RADS, VSQRT, VSIN, VCOS, VTAN,
2                VASIN, VACOS, VATAN);
2 FORMAT F(I2,2F14.8,6F15.8,W2),
2        HEAD(* D  RADS*,B10,*SQRT*,B11,*SIN*,B12,*COS*,B12,*TAN*,
2             B12,*ASIN*,B11,*ACOS*,B11,*ATAN*,W2);
2
2 TORADS = 3.1415926/180;
2 WRITE(;; HEAD);
2
2 FOR DEGREES = (1, 1, 89);
2   BEGIN
2   RADS = DEGREES.TORADS;
2   VSQRT = SQRT(RADS);
2   VSIN =  SIN(RADS);
2   VCOS =  COS(RADS);
2   VTAN =  TAN(RADS);
2   VASIN = ARCSIN(VSIN);
2   VACOS = ARCCOS(VCOS);
2   VATAN = ARCTAN(VTAN);
2   WRITE(;; RESULTS, F);
2   END;
2 FINISH;
